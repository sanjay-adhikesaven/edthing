{
  "name": "edthing",
  "version": "1.0.0",
  "description": "Student participation documentation and credit system",
  "scripts": {
    "dev": "cd web && npm run dev",
    "build": "cd web && npm run build",
    "start": "cd web && npm start",
    "ingest": "cd ingest && python -m ingest.sync",
    "ingest:manual": "cd ingest && python -m ingest.sync --manual",
    "db:migrate": "cd db && python migrate.py",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
